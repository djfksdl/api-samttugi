<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="item">
	<select id="getItemList" parameterType="int" resultType="com.javaex.vo.ProductVo" >
		select productNo
				,productName
        		,price
        		,storage
        		,detail
        		,saveName
        		,best
				from product p join mcategory m
		where m.mcNo = #{no}
	</select>
</mapper>